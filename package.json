{
    "name": "smartnshine-server",
    "version": "1.0.0",
    "description": "Backend API for SmartNShine Resume Builder",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "dev": "node --env-file=.env --watch server.js",
        "start": "node server.js",
        "create-admin": "node --env-file=.env scripts/createAdmin.js",
        "sync:templates": "node --env-file=.env scripts/syncTemplates.js",
        "check:templates": "node --env-file=.env scripts/checkTemplates.js",
        "find:duplicates": "node --env-file=.env scripts/listDuplicateTemplates.js",
        "dedupe:templates": "node --env-file=.env scripts/dedupeTemplates.js",
        "migrate:resume-titles": "node --env-file=.env scripts/migrateResumeTitles.js"
    },
    "keywords": [
        "resume",
        "ats",
        "ai",
        "gemini"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.21.0",
        "axios": "^1.7.9",
        "bcryptjs": "^2.4.3",
        "connect-mongo": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^8.1.0",
        "express-session": "^1.18.2",
        "express-validator": "^7.3.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "mammoth": "^1.8.0",
        "mongoose": "^8.9.3",
        "multer": "^1.4.5-lts.1",
        "node-fetch": "^3.3.2",
        "nodemailer": "^7.0.10",
        "passport": "^0.7.0",
        "passport-github2": "^0.1.12",
        "passport-google-oauth20": "^2.0.0",
        "pdf-parse": "1.1.1",
        "pdf.js-extract": "^0.2.1",
        "validator": "^13.15.20",
        "xss-clean": "^0.1.4"
    }
}